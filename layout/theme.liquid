    {%- if settings.favicon != blank -%}
    <link rel="icon" type="image/png" href="{{ settings.favicon | img_url: '32x32' }}">
    {%- endif -%}
    {% render 'mf-canonical' %}

    <link rel="preconnect" href="https://fonts.shopifycdn.com" crossorigin>

    <title>
      {{ page_title }}
      {%- if current_tags %} &ndash; tagged "{{ current_tags | join: ', ' }}"{% endif -%}
      {%- if current_page != 1 %} &ndash; Page {{ current_page }}{% endif -%}
      {%- unless page_title contains shop.name %} &ndash; {{ shop.name }}{% endunless -%}
    </title>

    {%- render "root" -%}
    {%- render "routes" -%}

    {% if page_description %}
    <meta name="description" content="{{ page_description | escape }}">
    {% endif %}

    {%- render 'stylesheet-loader' -%}

    {%- render 'meta-tags' -%}

    {{ content_for_header }}

    {%- render 'css-variables' -%}
    {%- section 'merchant-scripts-head' -%}
  
{% render 'gsf-conversion-pixels' %}
</head>
   
  <body class="resource-{{ request.page_type }}">

    <div data-header>
      {%- section 'static-announcement-bar' -%}
      {%- section 'static-header' -%}
      
      {%- render 'message-product-added' -%}
    </div>

    {%- render 'custom-props' -%}

    <main class="content-for-layout">
      {{ content_for_layout }}
    </main>

    {%- section 'USP' -%}
    {%- section 'static-footer' -%}

    {%- section "static-nav-drawer" -%}
    {%- render "search-drawer" -%}

    {%- unless request.page_type == 'cart' -%}
    {%- render "cart-drawer" -%}
    {%- endunless -%}

   {%- render 'money-helper' -%}

    <script src="{{ 'lazysizes.js' | asset_url }}" defer></script>
    <script src="{{ 'swiper.min.js' | asset_url }}" defer></script>
    

    <script src="{{ 'main.js' | asset_url }}" defer></script>

    {%- if request.page_type == "product" -%}
    <script src="{{ 'template-product.js' | asset_url }}" defer></script>
    {%- endif -%}
    
    {%- if request.page_type == "article" -%}
    <script src="{{ 'template-article.js' | asset_url }}" defer></script>
    {%- endif -%}
    
    {%- if request.page_type == "blog" -%}
    <script src="{{ 'template-blog.js' | asset_url }}" defer></script>
    {%- endif -%}

    {%- render "success-popup" -%}

    {%- render 'schema-json-ld' -%}
    
	{%- section 'merchant-scripts-body' -%}
  
{% if product %}
<script type="text/javascript">var _learnq = _learnq || [];  var classname = document.getElementsByClassName("button button--full");var addToCart = function() {_learnq.push(['track', 'Added to Cart', item]);}; for (var i = 0; i < classname.length; i++) {classname[i].addEventListener('click', addToCart, false);}</script>
{% endif %} 

